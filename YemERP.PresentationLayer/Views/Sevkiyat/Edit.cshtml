                        <label asp-for="SIPSIRANO"></label>
                        <input asp-for="SIPSIRANO" class="form-control" />
                        <span asp-validation-for="SIPSIRANO" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="MALNO"></label>
                        <input asp-for="MALNO" class="form-control" />
                        <span asp-validation-for="MALNO" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="MIKTAR"></label>
                        <input asp-for="MIKTAR" class="form-control" />
