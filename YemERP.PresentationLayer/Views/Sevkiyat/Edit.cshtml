                        <label asp-for="INCKEYNO"></label>
                        <input asp-for="INCKEYNO" class="form-control" />
                        <span asp-validation-for="INCKEYNO" class="text-danger"></span>
                    </div>

                    <div class="form-group">
                        <label asp-for="URETILDI"></label>
                        <input asp-for="URETILDI" class="form-control" />
                        <span asp-validation-for="URETILDI" class="text-danger"></span>
                    </div>

                    <div class="form-group float-right">
                        <button type="submit" value="Create" class="btn btn-primary">Sevkiyatı Düzenle</button>
